{
    "index_patterns": [
        "claude-proxy-*"
    ],
    "settings": {
        "number_of_shards": 1
    },
    "mappings": {
        "properties": {
            "ts": {
                "type": "date"
            },
            "level": {
                "type": "keyword"
            },
            "service": {
                "type": "keyword"
            },
            "request_id": {
                "type": "keyword"
            },
            "trace_id": {
                "type": "keyword"
            },
            "span_id": {
                "type": "keyword"
            },
            "user_id": {
                "type": "keyword"
            },
            "route": {
                "type": "keyword"
            },
            "method": {
                "type": "keyword"
            },
            "status": {
                "type": "integer"
            },
            "latency_ms": {
                "type": "scaled_float",
                "scaling_factor": 100
            },
            "message": {
                "type": "text"
            },
            "file": {
                "type": "keyword"
            },
            "line": {
                "type": "integer"
            }
        }
    }
}